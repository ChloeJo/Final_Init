<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace ="Feed">
	<insert id="write" parameterType="kh.init.feeds.FeedDTO">
		insert into tb_feed values (feed_seq.nextval, #{email}, #{nickname}, #{title}, #{contents}, #{relation}, sysdate) 
	</insert>
	
	<select id="selectAll" resultType="kh.init.feeds.FeedDTO">
		select * from tb_feed
	</select>
</mapper>